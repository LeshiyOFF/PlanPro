  "ignoreDependencies": [
    "concurrently",
    "wait-on",
    "cross-env",
    "electron-builder",
    "png-to-ico",
    "sharp",
    "@storybook/addon-essentials",
    "@storybook/addon-docs",
    "@storybook/addon-controls",
    "@storybook/addon-viewport",
    "eslint-plugin-storybook",
    "@oxc-resolver/binding-win32-x64-msvc",
    "@radix-ui/react-dropdown-menu",
