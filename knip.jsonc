    "src/components/dashboard/ProjectDashboard.tsx",
    "src/components/projects/ProjectCard.tsx",
    "src/components/projects/ProjectForm.tsx",
    "src/components/projects/ProjectManager.tsx",
    "src/components/resources/ResourceCard.tsx",
    "src/components/resources/ResourceForm.tsx",
    "src/components/resources/ResourceManager.tsx",
    "src/components/status/StatusHistory.tsx",
    "src/components/status/StatusMonitor.tsx",
    "src/components/statusbar/index.ts",
    "src/components/statusbar/StatusBar.tsx",
    "src/components/statusbar/StatusBarContainer.tsx",
    "src/components/statusbar/hooks/useStatusBar.ts",
    "src/components/statusbar/sections/index.ts",
